{
  "name": "encontrar",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint-staged-husky": "lint-staged",
    "tsc": "turbo tsc",
    "test": "turbo test -- --passWithNoTests",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "clean": "npm exec --workspaces -c 'rm -rf node_modules .next .turbo coverage dist compiled build' && rm -rf node_modules",
    "remove-turbo-cache": "rm -rf ./node_modules/.cache/turbo",
    "update-dependencies": "turbo update-dependencies && npx npm-check-updates -u"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.11.3",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.0",
    "@hookform/resolvers": "3.3.4",
    "@mui/icons-material": "5.15.14",
    "@mui/material": "5.15.14",
    "@react-oauth/google": "^0.12.1",
    "@reduxjs/toolkit": "^2.5.1",
    "@tanstack/react-query": "5.28.8",
    "@tanstack/react-query-devtools": "5.28.8",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "classnames": "^2.5.1",
    "date-fns": "3.6.0",
    "extenso": "^2.1.0",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "next-translate": "^2.6.2",
    "next-translate-plugin": "^2.6.2",
    "nookies": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.4.0",
    "react-slick": "^0.30.3",
    "react-toastify": "^11.0.3",
    "rollup": "^4.34.7",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.2.5",
    "swr": "^2.3.0",
    "yup": "^1.6.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.11.30",
    "@types/react": "18.2.70",
    "@types/react-dom": "18.2.22",
    "@types/react-slick": "0.23.13",
    "autoprefixer": "10.4.19",
    "commitizen": "4.3.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.12",
    "tailwindcss": "3.4.1",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsup": "8.0.2",
    "turbo": "1.13.0",
    "typescript": "5.4.3"
  },
  "packageManager": "npm@10.2.0",
  "engines": {
    "node": ">=20.11.1",
    "npm": ">=10.2.4"
  }
}
